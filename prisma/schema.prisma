  AL  @map("Alagoas")
  AP  @map("Amapá")
  AM  @map("Amazonas")
  BA  @map("Bahia")
  CE  @map("Ceará")
  DF  @map("Distrito Federal")
  ES  @map("Espírito Santo")
  GO  @map("Goías")
  MA  @map("Maranhão")
  MT  @map("Mato Grosso")
  MS  @map("Mato Grosso do Sul")
  MG  @map("Minas Gerais")
  PA  @map("Pará")
  PB  @map("Paraíba")
  PR  @map("Paraná")
  PE  @map("Pernambuco")
  PI  @map("Piauí")
  RJ  @map("Rio de Janeiro")
  RN  @map("Rio Grande do Norte")
  RS  @map("Rio Grande do Sul")
  RO  @map("Rondônia")
  RR  @map("Roraíma")
  SC  @map("Santa Catarina")
  SP  @map("São Paulo")
  SE  @map("Sergipe")
  TO  @map("Tocantins")
}

enum Model {
  SUNTECH
  GT06
}

enum MobileOperator {
  Claro
  Oi
  Tim
  Vivo
  Vodafone
  Outras
}

enum Timezone {
  GMT_14  @map("GMT-14")
  GMT_13  @map("GMT-13")
  GMT_12  @map("GMT-12")
  GMT_11  @map("GMT-11")
  GMT_10  @map("GMT-10")
  GMT_9   @map("GMT-9")
  GMT_8   @map("GMT-8")
  GMT_7   @map("GMT-7")
  GMT_6   @map("GMT-6")
  GMT_5   @map("GMT-5")
  GMT_4   @map("GMT-4")
  GMT_3   @map("GMT-3")
  GMT_2   @map("GMT-2")
  GMT_1   @map("GMT-1")
  GMT1    @map("GMT+1")
  GMT2    @map("GMT+2")
  GMT3    @map("GMT+3")
  GMT4    @map("GMT+4")
  GMT5    @map("GMT+5")
  GMT6    @map("GMT+6")
  GMT7    @map("GMT+7")
  GMT8    @map("GMT+8")
  GMT9    @map("GMT+9")
  GMT10   @map("GMT+10")
  GMT11   @map("GMT+11")
  GMT12   @map("GMT+12")
}
